type Bidder {
    id: String!
    firstName: String!
    lastName: String!
}

type Product{
    id: String!
    name: String!
    description: String!
    address: String!
    price: Float!
}

type Auction {
    id: String!
    status: Status!
    productId: String!
    targetPrice: Float!
    reservePrice: Float
}

type Bid {
    id: String!
    auctionId: String!
    bidderId: String!
    amount: Float!
    bidStatus: BidStatus
}

